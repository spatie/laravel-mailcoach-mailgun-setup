{
    "name": "spatie/laravel-mailcoach-mailgun-setup",
    "description": "Setup Mailgun for use with Mailcoach",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-mailcoach-mailgun-setup"
    ],
    "homepage": "https://github.com/spatie/laravel-mailcoach-mailgun-setup",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachMailgunSetup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachMailgunSetup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "Spatie\\MailcoachMailgunSetup\\MailcoachMailgunSetupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
